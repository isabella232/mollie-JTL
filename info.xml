  <Install>
    <Version nr="100">
      <SQL>100.sql</SQL>
      <CreateDate>2019-02-11</CreateDate>
    </Version>
    <Version nr="101">
      <CreateDate>2019-02-26</CreateDate>
    </Version>
    <Version nr="102">
      <CreateDate>2019-04-23</CreateDate>
    </Version>
    <Version nr="103">
      <CreateDate>2019-05-22</CreateDate>
    </Version>
    <Version nr="104">
      <CreateDate>2019-07-12</CreateDate>
